<div id="page-wrapper">

  <div class="row">
    <div class="col-lg-12">
      <h1>All Orders <small>all orders in the system</small></h1>
      <ol class="breadcrumb">
        <li><i class="fa fa-dashboard"></i> Dashboard</li>
        <li><i class="fa fa-dashboard"></i> All Orders</li>
      </ol>
      
    </div>
  </div><!-- /.row -->

  <table class="table table-hover">
    <tr>
        <th>Order #</th>

        <th>Customer</th>

        <th>Status</th>

        <th>Order Total</th>
        
        <th>Order placed at</th>

        <th>Delete</th>
    </tr>
    <% cache @orders do %>
      <%= render @orders %> 
    <% end %>
  </table>

</div><!-- /#page-wrapper -->
