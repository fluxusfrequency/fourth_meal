<div id="page-wrapper">

  <div class="row">
    <div class="col-lg-12">
      <h1>All Restaurants <small>All Restaurants In The System</small></h1>
      <ol class="breadcrumb">
        <li><i class="fa fa-dashboard"></i> Dashboard</li>
        <li><i class="fa fa-dashboard"></i> All Restaurants</li>
      </ol>
      
    </div>
  </div><!-- /.row -->

  <table class="table table-hover">
    <tr>
      <th>ID #</th>

      <th>Title</th>

      <th>Description</th>

      <th>Url</th>
      
      <th>Owners</th>

      <th>Date Created</th>

      <th>Status</th>

      <th>Active?</th>

      <th>Approve</th>

      <th>Reject</th>

      <th>Turn Off/On</th>

      <th>Administer</th>
    </tr>
    <% cache @restaurants do %>
      <%= render @restaurants %>
    <% end %>
  </table>

</div><!-- /#page-wrapper -->
