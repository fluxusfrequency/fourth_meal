<div class="grid-30 address-panel">
  <li class="list">
    <%= address.first_name + " " + address.last_name + "2312412" %><br/>
    <%= address.street_address %><br/>
    <%= address.city + ", " + address.state %><br/>
    <%= address.zipcode %><br/>
    <%= address.email %><br/>
    <%= link_to "Use Address", new_transaction_path(session[:current_restaurant], address_id: address.id), class: "button", id:"transaction-button" %>
  </li><br/><br/><br/>
</div>

